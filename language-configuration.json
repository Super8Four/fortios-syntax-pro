{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["config", "end"],
        ["edit", "next"],
        ["(", ")"],
        ["[", "]"],
        ["{", "}"]
    ],
    "autoClosingPairs": [
        ["config", "end"],
        ["edit", "next"],
        ["\"", "\""],
        ["'", "'"],
        ["(", ")"],
        ["[", "]"],
        ["{", "}"]
    ],
    "surroundingPairs": [
        ["config", "end"],
        ["edit", "next"],
        ["\"", "\""],
        ["'", "'"],
        ["(", ")"],
        ["[", "]"],
        ["{", "}"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "(?i)^(.*\\b(config|edit)\\b.*[^{])$",
        "decreaseIndentPattern": "(?i)^\\s*(\\bend\\b|\\bnext\\b).*"
    },
    "wordPattern": "/\\b(\\w+(-\\w+)*)\\b/"
}